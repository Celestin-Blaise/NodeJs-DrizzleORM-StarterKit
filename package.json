{
  "name": "bytedash",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup": "npm install -g typescript tsc tsc-alias pm2",
    "production": "pm2-runtime start dist/src/server.js --watch",
    "dev:prod": "nodemon dist/src/server.js",
    "dev": "tsx --watch src/server.ts",
    "build": "tsc && tsc-alias",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "cross-env DB_MIGRATING=true tsx src/db/migrate.ts",
    "db:seed": "cross-env DB_SEEDING=true tsx src/db/seed.ts",
    "studio": "npx drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "argon2": "^0.40.1",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-kit": "^0.21.4",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "typed-rpc": "^5.0.2",
    "zod": "^3.23.8",
    "typescript": "^5.4.5",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.10.5"
  }
}
